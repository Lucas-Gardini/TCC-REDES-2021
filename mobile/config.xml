<?xml version='1.0' encoding='utf-8'?>
<widget id="app.fastcoders.commander" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Commander-Mobile</name>
    <description>
        A cordova aplication that integrates with a restaurant control platform, see: https://github.com/Lucas-Gardini/TCC-REDES-2021
    </description>
    <author email="lucasgardini.dias@hotmail.com">
        Fast Coders
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
